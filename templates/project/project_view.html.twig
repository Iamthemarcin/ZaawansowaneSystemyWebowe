                                    </div>
                                    <script src="{{ asset('assets/js/chart.js') }}"></script>
                                    <script src="{{ asset('assets/js/speedTest.js') }}"></script>
                                    <script src="{{ asset('assets/js/minuteTest.js') }}"></script>

