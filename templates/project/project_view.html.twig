                                                        <span class="dropdown-item">5.3</span>
                                                        <span class="dropdown-item">Najnowsza wersja WordPress</span>
                                                        <span class="dropdown-item">5.3.1</span>
                                                    </div>
                                                </div>
                                            </div>
                                        </td>
                                    </tr>
                                    </tbody>
                                </table>

                                <!-- /basic responsive configuration -->
                            </div>

                            <!-- /Zakladka 4 -->

                        </div>
                    </div>
                </div>
            </div>


        </div>
        <!-- /rounded basic tabs -->


    </div>
    <!-- /content area -->

{% endblock %}